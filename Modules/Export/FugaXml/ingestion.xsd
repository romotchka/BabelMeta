                                      </xsd:documentation>
                                    </xsd:annotation>
                                  </xsd:element>
                                  <xsd:element name="key" maxOccurs="1" minOccurs="0">
                                    <xsd:annotation>
                                      <xsd:documentation>The key of this classical track.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                      <xsd:restriction base="xsd:String">
                                        <xsd:enumeration value="A_MAJOR"/>
                                        <xsd:enumeration value="A_MINOR"/>
                                        <xsd:enumeration value="A_FLAT_MAJOR"/>
                                        <xsd:enumeration value="B_FLAT_MAJOR"/>
                                        <xsd:enumeration value="B_FLAT_MINOR"/>
                                        <xsd:enumeration value="B_MAJOR"/>
                                        <xsd:enumeration value="B_MINOR"/>
                                        <xsd:enumeration value="C_MAJOR"/>
                                        <xsd:enumeration value="C_MINOR"/>
                                        <xsd:enumeration value="C_SHARP_MINOR"/>
                                        <xsd:enumeration value="D_MAJOR"/>
                                        <xsd:enumeration value="D_MINOR"/>
                                        <xsd:enumeration value="D_SHARP_MINOR"/>
                                        <xsd:enumeration value="D_FLAT_MAJOR"/>
