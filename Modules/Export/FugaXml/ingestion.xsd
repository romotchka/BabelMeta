                                        <xsd:enumeration value="F_SHARP_MINOR"/>
                                        <xsd:enumeration value="G_MAJOR"/>
                                        <xsd:enumeration value="G_MINOR"/>
                                        <xsd:enumeration value="G_FLAT_MAJOR"/>
                                        <xsd:enumeration value="G_SHARP_MINOR"/>
