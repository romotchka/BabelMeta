                        </xsd:element>                        
                        <xsd:element name="classical_track" >
                            <xsd:complexType>
                                <xsd:annotation>
                                    <xsd:documentation>Element describing one classical track of an album.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:all>
                                    <xsd:element name="allow_preorder_preview" type="xsd:boolean" maxOccurs="1" minOccurs="0">
