                                            </xsd:documentation>
                                        </xsd:annotation>
                                        <xsd:simpleType>
                                            <xsd:restriction base="xsd:String">
                                                <xsd:pattern value="[A-Z]{2}[A-Z0-9]{3}[0-9]{7}"/>
